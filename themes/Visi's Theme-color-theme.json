				"keyword.other",
				"keyword.control",
				"meta.head.struct",
				"storage.type.namespace.directive",
				"storage.modifier",
				"keyword.operator.new",
				"storage.type.class",
				"storage.type.function",
				"storage.type.struct",
				"storage.type.namespace",
				"storage.type.interface",
				"storage.type.enum",
				"storage.control",
                "entity.name.tag.css",
                "constant.other.symbol.elixir",
                "keyword.fsharp",
                "keyword.function",
                "keyword.interface",
                "keyword.map",
                "keyword.struct",
                "keyword.channel",
