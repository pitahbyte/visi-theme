				"punctuation.section.angle-brackets.end.template.call.cpp",
                "entity.name.tag.reference",
                "punctuation.definition.entity",
                "entity.name.tag.wildcard",
                "punctuation.separator.method",
                "punctuation.definition.constant.elixir",
                "keyword.symbol",
                "punctuation.separator.statement.and",
                "keyword.control.clause.bodybegin",
                "punctuation.definition.variable.elixir",
                "punctuation.math.operator.latex",
                "entity.name.function.member.overload",
                "keyword.control.if.prolog",
                "keyword.control.else.prolog",
                "keyword.control.cut.prolog",
            ],
			"settings": {
				"foreground": "#ff5572"
			}
